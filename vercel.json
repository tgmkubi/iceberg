{
    "version": 2,
    "functions": {
        "api/index.ts": {
            "maxDuration": 30,
            "memory": 1024,
            "runtime": "nodejs20.x"
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/index"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "regions": [
        "fra1"
    ]
}